<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RotatePointer">
        <attr name="rotateImageDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="Progress_Arc">
        <attr name="startAngle" format="float" />
        <attr name="sweepAngle" format="float" />
        <attr name="firstsweepAngle" format="float"/>
        <attr name="secondsweepAngle" format="float"/>
        <attr name="space" format="float"/>
        <attr name="firstspinColor" format="color" />
        <attr name="secondspinColor" format="color" />
        <attr name="barWidth" format="dimension"/>
        <attr name="barColor" format="color"/>
        <attr name="spinWidth" format="dimension"/>
        <attr name="spinHeight" format="dimension"/>
        <attr name="circleColor" format="color"/>
        <attr name="borderColor" format="color"/>
        <attr name="borderWidth" format="dimension"/>
        <attr name="progress" format="float"/>
        <attr name="clockwise" format="boolean"/>
    </declare-styleable>


    <!--扩散圆颜色-->
    <attr name="diffuse_color" format="color"/>
    <!--中心圆颜色-->
    <attr name="diffuse_coreColor" format="color"/>
    <!--中心圆图片-->
    <attr name="diffuse_coreImage" format="reference"/>
    <!--中心圆半径-->
    <attr name="diffuse_coreRadius" format="float"/>
    <!--扩散圆宽度,值越小越宽-->
    <attr name="diffuse_width" format="integer"/>
    <!--最大扩散宽度-->
    <attr name="diffuse_maxWidth" format="integer"/>

    <declare-styleable name="DiffuseView">
        <attr name="diffuse_color"/>
        <attr name="diffuse_coreColor"/>
        <attr name="diffuse_coreImage"/>
        <attr name="diffuse_coreRadius"/>
        <attr name="diffuse_width"/>
        <attr name="diffuse_maxWidth"/>
    </declare-styleable>

</resources>